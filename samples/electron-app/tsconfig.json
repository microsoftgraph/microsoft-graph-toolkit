  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es6",
    "resolveJsonModule": true,
    "sourceMap": true,
    "removeComments": false,
    "outDir": "dist",
    "noImplicitReturns": true,
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["src/main/*"]
}
