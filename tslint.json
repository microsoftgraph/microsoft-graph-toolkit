{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "interface-name": false,
    "completed-docs": [
      true,
      {
        "methods": {
          "privacies": ["public", "protected"]
        },
        "properties": {
          "privacies": ["public", "protected"]
        },
        "classes": true,
        "enums": true,
        "enum-members": true,
        "functions": true,
        "interfaces": true,
        "namespaces": true,
        "types": true,
        "variables": true
      }
    ],
    "variable-name": {
      "options": ["allow-leading-underscore"]
    }
  },
  "rulesDirectory": []
}
